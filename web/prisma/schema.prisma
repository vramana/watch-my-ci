generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model workflow {
  id    Int    @unique
  repo  String  
  name  String  
  path  String 
}

model workflowRun {
  id          BigInt    @unique
  duration    Int
  success     Boolean 
  workflowId  Int
  repo        String
}